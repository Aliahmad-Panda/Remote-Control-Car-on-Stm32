/ {
	load_switch: load_switch {
		compatible = "power-switch";
		/* using built-in LED pin for demonstration */
		gpios = <&gpioa 5 GPIO_ACTIVE_HIGH>;
	}; 
};
/ {
	another_switch: another_switch {
		compatible = "power-switch";
		/* using built-in LED pin for demonstration */
		gpios = <&gpioa 4 GPIO_ACTIVE_HIGH>;
	}; 
};
